<% include ./partials/header.ejs %>
<h1>Bucket List</h1>
  <% items.forEach(item => { %>
    <h2>
      <%= item.id %>
        <%= item.item_name %>
        <%= item.done %>
    </h2>
    <form class="new-item-form" method="POST" action="/homepage">
      <label>Activity</label>
      <input name="activity" type="text">
      <label>Done</label>
      <input name="done" type="boolean" />
      <button type="submit">Add to List </button>
    </form>
    <% }); %>
      <% include ./partials/footer.ejs %>